<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">

    {% block stylesheets %}
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@5.3.1/dist/cerulean/bootstrap.min.css"
              integrity="sha256-sbMLJcN5yZVmLCbHZ/pGf9hLFtXD7GvBJ8/bvO+OF3M=" crossorigin="anonymous">

    {% endblock %}
    {% block stylesheets_custom %}
        <link rel="stylesheet" href="{{ asset('css/form.css') }}">
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('css/media.css') }}">
    {% endblock %}


    {% block javascripts %}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
                crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"></script>

    {% endblock %}
</head>
<body>
{% block body %}
    <div class="container-fluid py-1 h-100">
        <div class="row h-100">
            <div class="col-2 frameLeft">

                {% include("partials/sidemenu.html.twig") %}
            </div>
            <div class="col-md col-6 frameRight">
                {% block right %}
                {% endblock %}
            </div>
        </div>
    </div>


{% endblock %}

{% block extrajs %}
{% endblock %}
</body>
</html>